{
	"name": "kitify",
	"version": "0.4.0",
	"description": "kitify is a JavaScript utility library that provides a whole mess of useful helper functions and supports modularity.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"unpkg": "dist/index.min.js",
	"jsdelivr": "dist/index.min.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"module": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./assign": {
			"types": "./dist/assign.d.ts",
			"import": "./dist/assign.mjs",
			"module": "./dist/assign.mjs",
			"require": "./dist/assign.js"
		},
		"./clone": {
			"types": "./dist/clone.d.ts",
			"import": "./dist/clone.mjs",
			"module": "./dist/clone.mjs",
			"require": "./dist/clone.js"
		},
		"./cloneDeep": {
			"types": "./dist/cloneDeep.d.ts",
			"import": "./dist/cloneDeep.mjs",
			"module": "./dist/cloneDeep.mjs",
			"require": "./dist/cloneDeep.js"
		},
		"./cloneLoop": {
			"types": "./dist/cloneLoop.d.ts",
			"import": "./dist/cloneLoop.mjs",
			"module": "./dist/cloneLoop.mjs",
			"require": "./dist/cloneLoop.js"
		},
		"./cloneJSON": {
			"types": "./dist/cloneJSON.d.ts",
			"import": "./dist/cloneJSON.mjs",
			"module": "./dist/cloneJSON.mjs",
			"require": "./dist/cloneJSON.js"
		},
		"./detectMouseDirection": {
			"types": "./dist/detectMouseDirection.d.ts",
			"import": "./dist/detectMouseDirection.mjs",
			"module": "./dist/detectMouseDirection.mjs",
			"require": "./dist/detectMouseDirection.js"
		},
		"./isType": {
			"types": "./dist/isType.d.ts",
			"import": "./dist/isType.mjs",
			"module": "./dist/isType.mjs",
			"require": "./dist/isType.js"
		},
		"./listToTree": {
			"types": "./dist/listToTree.d.ts",
			"import": "./dist/listToTree.mjs",
			"module": "./dist/listToTree.mjs",
			"require": "./dist/listToTree.js"
		},
		"./treeToList": {
			"types": "./dist/treeToList.d.ts",
			"import": "./dist/treeToList.mjs",
			"module": "./dist/treeToList.mjs",
			"require": "./dist/treeToList.js"
		},
		"./type": {
			"types": "./dist/type.d.ts",
			"import": "./dist/type.mjs",
			"module": "./dist/type.mjs",
			"require": "./dist/type.js"
		},
		"./*": "./dist/*"
	},
	"files": [
		"dist",
		"docs"
	],
	"scripts": {
		"build": "node ../scripts/build.mjs"
	},
	"keywords": [
		"kitify",
		"kit",
		"utility",
		"functions",
		"lodash"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"sideEffects": false,
	"homepage": "https://github.com/Marinerer/kitify",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Marinerer/kitify.git"
	},
	"author": "Mariner <mengqing723@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Marinerer/kitify/issues"
	}
}
