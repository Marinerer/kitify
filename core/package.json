{
	"name": "kitify",
	"version": "0.5.2",
	"description": "A JavaScript utility library that provides a whole mess of useful helper functions and supports modularity.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"unpkg": "dist/index.min.js",
	"jsdelivr": "dist/index.min.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./type": {
			"import": {
				"types": "./dist/type.d.mts",
				"default": "./dist/type.mjs"
			},
			"require": {
				"types": "./dist/type.d.ts",
				"default": "./dist/type.js"
			}
		},
		"./package.json": "./package.json",
		"./*": "./*"
	},
	"files": [
		"dist",
		"docs"
	],
	"scripts": {
		"dev": "rollup --config rollup.config.mjs --watch",
		"build": "node ./build.mjs",
		"build:lib": "rollup --config rollup.config.mjs",
		"build:deprecated": "node ../scripts/build.mjs"
	},
	"keywords": [
		"kitify",
		"kit",
		"util",
		"functions",
		"modules",
		"lodash",
		"utility"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"sideEffects": false,
	"homepage": "https://github.com/Marinerer/kitify",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Marinerer/kitify.git"
	},
	"author": "Mariner <mengqing723@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Marinerer/kitify/issues"
	}
}
